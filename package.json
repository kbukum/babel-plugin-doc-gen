{
  "name": "babel-plugin-doc-gen",
  "version": "0.0.17",
  "description": "React Doc Generator and JSDoc Generator",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha __test__ --recursive --compilers js:babel-core/register",
    "docs": "esdoc -c esdoc.json",
    "build": "webpack --config webpack.config.build.js  --progress --profile --colors && babel ./src --out-dir ./lib"
  },
  "keywords": [
    "jsdocs-gen",
    "react-doc-gen",
    "babel",
    "webpack",
    "es6",
    "eslint",
    "flowtype",
    "karma",
    "chai",
    "mocha",
    "isparta",
    "istanbul",
    "esdocs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbukum/babel-plugin-doc-gen.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-flowtype": "^2.3.1",
    "eslint-plugin-import": "^1.11.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "webpack": "^1.13.1"
  },
  "bugs": {
    "url": "https://github.com/kbukum/babel-plugin-doc-gen/issues"
  },
  "homepage": "https://github.com/kbukum/babel-plugin-doc-gen#readme",
  "dependencies": {
    "fs": "0.0.2",
    "fs-extra": "^0.30.0",
    "graceful-fs": "^4.1.4",
    "react-docgen": "^2.9.1"
  },
  "author": "Kamil Bukum <kamilbukum@gmail.com> (oopdev.org)"
}
