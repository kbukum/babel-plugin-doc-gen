{
  "name": "babel-plugin-doc-gen",
  "version": "0.0.21",
  "description": "React Doc Generator and JSDoc Generator",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha src --recursive  --no-timeouts --compilers js:babel-register",
    "docs": "esdoc -c esdoc.json",
    "build": "webpack --config webpack.config.build.js  --progress --profile --colors && babel ./src --out-dir ./lib  --ignore *.spec.js"
  },
  "keywords": [
    "jsdocs-gen",
    "react-doc-gen",
    "babel",
    "webpack",
    "es6",
    "eslint",
    "flowtype",
    "karma",
    "chai",
    "mocha",
    "isparta",
    "istanbul",
    "esdocs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbukum/babel-plugin-doc-gen.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "esdoc": "0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "3.11.1",
    "eslint-import-resolver-webpack": "0.7.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-flowtype": "2.29.1",
    "eslint-plugin-import": "2.2.0",
    "file-loader": "0.9.0",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "webpack": "1.13.3"
  },
  "bugs": {
    "url": "https://github.com/kbukum/babel-plugin-doc-gen/issues"
  },
  "homepage": "https://github.com/kbukum/babel-plugin-doc-gen#readme",
  "dependencies": {
    "fs-extra": "1.0.0",
    "graceful-fs": "4.1.10",
    "react-docgen": "2.12.1"
  },
  "author": "Kamil Bukum <kamilbukum@gmail.com> (oopdev.org)"
}
